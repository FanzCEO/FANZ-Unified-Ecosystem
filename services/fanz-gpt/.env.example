# ðŸ¤– FanzGPT AI Assistant - Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# AI PROVIDER API KEYS
# =============================================================================

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ORG=org-your-openai-organization-id
OPENAI_PROJECT=proj_your-openai-project-id

# Anthropic Configuration
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Additional AI Providers (Optional)
HUGGINGFACE_API_KEY=hf_your-huggingface-token
COHERE_API_KEY=your-cohere-api-key
REPLICATE_API_TOKEN=r8_your-replicate-token

# =============================================================================
# DATABASE & STORAGE
# =============================================================================

# PostgreSQL Database
DATABASE_URL=postgresql://fanzgpt_user:secure_password@localhost:5432/fanzgpt_db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=fanzgpt_db
DATABASE_USER=fanzgpt_user
DATABASE_PASSWORD=secure_password

# Redis Cache
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# AWS S3 Storage (for media files)
AWS_ACCESS_KEY_ID=AKIA_your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=fanzgpt-media-storage
S3_BUCKET_REGION=us-east-1

# =============================================================================
# SECURITY & AUTHENTICATION
# =============================================================================

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-min-32-characters
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Encryption Keys
ENCRYPTION_KEY=your-32-character-encryption-key-here
ENCRYPTION_ALGORITHM=aes-256-gcm

# API Security
API_RATE_LIMIT_MAX=100
API_RATE_LIMIT_WINDOW=900000
API_CORS_ORIGIN=https://fanz.com,https://app.fanz.com

# Session Management
SESSION_SECRET=your-session-secret-key-here
COOKIE_SECURE=true
COOKIE_HTTP_ONLY=true
COOKIE_SAME_SITE=strict

# =============================================================================
# CONTENT MODERATION & COMPLIANCE
# =============================================================================

# Content Moderation
CONTENT_MODERATION_STRICT=false
ADULT_CONTENT_ALLOWED=true
NSFW_DETECTION_ENABLED=true
AUTO_CONTENT_FLAGGING=true

# Compliance Settings
AGE_VERIFICATION_REQUIRED=true
MIN_USER_AGE=18
RECORD_KEEPING_ENABLED=true
GDPR_COMPLIANCE=true

# Custom Content Filters (comma-separated)
CUSTOM_CONTENT_FILTERS=extreme violence,hate speech,illegal content,spam

# =============================================================================
# AI MODEL CONFIGURATION
# =============================================================================

# OpenAI Models
OPENAI_MODEL_PRIMARY=gpt-4-turbo-preview
OPENAI_MODEL_FALLBACK=gpt-3.5-turbo
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7

# Anthropic Models
ANTHROPIC_MODEL_PRIMARY=claude-3-sonnet-20240229
ANTHROPIC_MODEL_FALLBACK=claude-3-haiku-20240307
ANTHROPIC_MAX_TOKENS=4000
ANTHROPIC_TEMPERATURE=0.7

# Voice Synthesis (OpenAI TTS)
VOICE_MODEL=tts-1-hd
VOICE_DEFAULT=alloy
VOICE_FORMAT=mp3
VOICE_SPEED=1.0

# Speech-to-Text (OpenAI Whisper)
STT_MODEL=whisper-1
STT_LANGUAGE=auto
STT_RESPONSE_FORMAT=json
STT_TEMPERATURE=0.0

# =============================================================================
# RATE LIMITING & QUOTAS
# =============================================================================

# OpenAI Rate Limits
OPENAI_REQUESTS_PER_MINUTE=50
OPENAI_TOKENS_PER_MINUTE=50000
OPENAI_REQUESTS_PER_DAY=10000
OPENAI_TOKENS_PER_DAY=1000000

# Anthropic Rate Limits
ANTHROPIC_REQUESTS_PER_MINUTE=40
ANTHROPIC_TOKENS_PER_MINUTE=40000
ANTHROPIC_REQUESTS_PER_DAY=8000
ANTHROPIC_TOKENS_PER_DAY=800000

# User Rate Limits
USER_REQUESTS_PER_HOUR=100
USER_TOKENS_PER_HOUR=10000
VIP_REQUESTS_PER_HOUR=500
VIP_TOKENS_PER_HOUR=50000

# =============================================================================
# CACHING CONFIGURATION
# =============================================================================

# Cache Settings
CACHE_ENABLED=true
CACHE_MAX_SIZE=1000
CACHE_MAX_AGE=3600000
CACHE_PREFIX=fanzgpt:
CACHE_COMPRESSION=true

# Content Cache TTL (in seconds)
CACHE_TTL_SOCIAL_POSTS=3600
CACHE_TTL_CHAT_RESPONSES=1800
CACHE_TTL_IMAGE_ANALYSIS=7200
CACHE_TTL_VOICE_SYNTHESIS=86400

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================

# Analytics
ANALYTICS_ENABLED=true
USAGE_TRACKING=true
PERFORMANCE_TRACKING=true
RETENTION_DAYS=90

# Error Tracking (Sentry)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ENVIRONMENT=production
SENTRY_TRACES_SAMPLE_RATE=0.1

# Monitoring
METRICS_ENABLED=true
METRICS_PORT=3001
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health

# Prometheus Metrics
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
PROMETHEUS_METRICS_PATH=/metrics

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================

# Log Levels: error, warn, info, debug, trace
LOG_LEVEL=info
LOG_FORMAT=json
LOG_TIMESTAMP=true
LOG_COLORIZE=false

# Log Destinations
LOG_TO_CONSOLE=true
LOG_TO_FILE=true
LOG_FILE_PATH=./logs/fanzgpt.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Core Features
FEATURE_CONTENT_GENERATION=true
FEATURE_CHAT_ASSISTANCE=true
FEATURE_IMAGE_ANALYSIS=true
FEATURE_VOICE_PROCESSING=true
FEATURE_CREATOR_TOOLS=true

# Experimental Features
FEATURE_VOICE_CLONING=false
FEATURE_VIDEO_ANALYSIS=false
FEATURE_CUSTOM_MODELS=false
FEATURE_ADVANCED_ANALYTICS=false

# Premium Features
FEATURE_PRIORITY_SUPPORT=false
FEATURE_CUSTOM_TRAINING=false
FEATURE_WHITE_LABEL=false
FEATURE_ENTERPRISE_SLA=false

# =============================================================================
# INTEGRATION SETTINGS
# =============================================================================

# FANZ Ecosystem Integration
FANZ_API_URL=https://api.fanz.com
FANZ_API_KEY=your-fanz-api-key
FANZ_WEBHOOK_SECRET=your-fanz-webhook-secret

# Platform Integrations (Optional)
INSTAGRAM_CLIENT_ID=your-instagram-client-id
INSTAGRAM_CLIENT_SECRET=your-instagram-client-secret
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
ONLYFANS_API_KEY=your-onlyfans-api-key
TIKTOK_CLIENT_KEY=your-tiktok-client-key

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================

# Environment
NODE_ENV=development
PORT=3000
HOST=localhost
CORS_ENABLED=true

# Development Tools
HOT_RELOAD=true
AUTO_RESTART=true
DEBUG_ENABLED=false
PROFILING_ENABLED=false

# Testing
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/fanzgpt_test
TEST_REDIS_URL=redis://localhost:6379/1
MOCK_AI_RESPONSES=false
SKIP_AI_CALLS=false

# =============================================================================
# EMAIL & NOTIFICATIONS
# =============================================================================

# SMTP Configuration (for notifications)
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=apikey
SMTP_PASS=your-sendgrid-api-key
FROM_EMAIL=noreply@fanz.com
FROM_NAME=FanzGPT AI Assistant

# Notification Settings
NOTIFICATIONS_ENABLED=true
EMAIL_NOTIFICATIONS=true
SLACK_WEBHOOK_URL=https://hooks.slack.com/your-webhook-url
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook

# =============================================================================
# PERFORMANCE & SCALING
# =============================================================================

# Concurrency
WORKER_PROCESSES=auto
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000
KEEP_ALIVE_TIMEOUT=5000

# Memory Management
MAX_MEMORY_USAGE=512MB
MEMORY_CHECK_INTERVAL=60000
GC_INTERVAL=300000

# Queue Management
QUEUE_ENABLED=true
QUEUE_MAX_SIZE=1000
QUEUE_CONCURRENCY=10
QUEUE_RETRY_ATTEMPTS=3
QUEUE_RETRY_DELAY=5000

# =============================================================================
# BACKUP & RECOVERY
# =============================================================================

# Backup Settings
BACKUP_ENABLED=false
BACKUP_INTERVAL=86400000
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=fanzgpt-backups

# Recovery Settings
AUTO_RECOVERY=true
RECOVERY_TIMEOUT=60000
HEALTH_CHECK_INTERVAL=30000

# =============================================================================
# LEGAL & COMPLIANCE URLS
# =============================================================================

# Legal Documents
PRIVACY_POLICY_URL=https://fanz.com/privacy
TERMS_OF_SERVICE_URL=https://fanz.com/terms
COOKIE_POLICY_URL=https://fanz.com/cookies
DMCA_POLICY_URL=https://fanz.com/dmca

# Compliance Contacts
COMPLIANCE_EMAIL=compliance@fanz.com
LEGAL_EMAIL=legal@fanz.com
DPO_EMAIL=dpo@fanz.com

# =============================================================================
# CUSTOM CONFIGURATION
# =============================================================================

# Custom Settings (add your own as needed)
CUSTOM_FEATURE_FLAG_1=false
CUSTOM_API_ENDPOINT=https://api.custom-service.com
CUSTOM_INTEGRATION_KEY=your-custom-integration-key

# =============================================================================
# NOTES
# =============================================================================

# Important Security Notes:
# 1. Never commit this file with real credentials to version control
