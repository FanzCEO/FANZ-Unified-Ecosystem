    "test:e2e": "jest --config jest.e2e.config.js --passWithNoTests",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "typecheck": "tsc --noEmit",
    "websocket:test": "node scripts/test-websocket.js",
    "moderation:test": "tsx scripts/test-moderation.ts",
    "streaming:test": "tsx scripts/test-streaming.ts",
