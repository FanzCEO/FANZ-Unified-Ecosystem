{
  "name": "@fanz/creator-crm",
  "version": "1.0.0",
  "description": "Comprehensive creator relationship management service for the FANZ ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "db:migrate": "npm run db:migrate:up",
    "db:migrate:up": "node -r ts-node/register scripts/migrate.ts up",
    "db:migrate:down": "node -r ts-node/register scripts/migrate.ts down",
    "db:seed": "node -r ts-node/register scripts/seed.ts",
    "analytics:update": "node -r ts-node/register scripts/update-analytics.ts",
    "compliance:check": "node -r ts-node/register scripts/check-compliance.ts",
    "campaigns:process": "node -r ts-node/register scripts/process-campaigns.ts"
  },
  "keywords": [
    "crm",
    "creator-management",
    "analytics",
    "onboarding",
    "earnings",
    "compliance",
    "campaigns",
    "marketing-automation",
    "fanz",
    "adult-content",
    "creator-economy"
  ],
  "author": "FANZ Engineering Team",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.8",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "nodemailer": "^7.0.7",
    "twilio": "^4.18.0",
    "axios": "^1.6.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "validator": "^13.15.15",
    "multer": "^2.0.2",
    "sharp": "^0.32.6",
    "pdf-lib": "^1.17.1",
    "handlebars": "^4.7.8",
    "node-cron": "^3.0.3",
    "bull": "^4.11.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.5",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.14.201",
    "@types/jest": "^29.5.8",
    "@types/nodemailer": "^6.4.14",
    "@types/validator": "^13.11.6",
    "@types/multer": "^1.4.11",
    "@types/sharp": "^0.32.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuastone/FANZ-Unified-Ecosystem.git",
    "directory": "services/creator-crm"
  },
  "bugs": {
    "url": "https://github.com/joshuastone/FANZ-Unified-Ecosystem/issues"
  },
  "homepage": "https://github.com/joshuastone/FANZ-Unified-Ecosystem#readme"
}
