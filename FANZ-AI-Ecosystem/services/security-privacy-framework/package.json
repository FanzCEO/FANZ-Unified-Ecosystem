{
  "name": "@fanz/security-privacy-framework",
  "version": "1.0.0",
  "description": "FANZ Advanced Security & Privacy Framework with zero-trust architecture, biometric authentication, and military-grade encryption",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:security": "npm test -- --testPathPattern=security",
    "test:privacy": "npm test -- --testPathPattern=privacy",
    "audit": "npm audit --audit-level=high",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "fanz",
    "security",
    "privacy",
    "zero-trust",
    "biometric-authentication",
    "end-to-end-encryption",
    "fraud-detection",
    "decentralized-identity",
    "military-grade-security"
  ],
  "author": "FANZ Security Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "node-forge": "^1.3.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "rate-limiter-flexible": "^3.0.8",
    "express-brute": "^1.0.1",
    "joi": "^17.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}