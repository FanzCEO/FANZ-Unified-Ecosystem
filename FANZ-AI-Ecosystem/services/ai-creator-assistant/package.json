{
  "name": "@fanz/ai-creator-assistant",
  "version": "1.0.0",
  "description": "AI-powered creator assistant with intelligent content generation, strategic recommendations, revenue optimization, and creative tools",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && copyfiles -u 1 src/**/*.json prompts/**/*.txt dist/",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ai-integration": "jest --testMatch='**/*ai-integration*.test.ts' --passWithNoTests",
    "test:api": "jest --testMatch='**/*.api.test.ts' --passWithNoTests",
    "test:content-quality": "jest --testMatch='**/*content-quality*.test.ts' --passWithNoTests",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:performance": "jest --testMatch='**/*performance*.test.ts' --passWithNoTests",
    "test:memory": "jest --testMatch='**/*memory*.test.ts' --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts' 'prompts/**/*.txt'",
    "clean": "rimraf dist",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "seed": "knex seed:run",
    "docker:build": "docker build -t fanz/ai-creator-assistant .",
    "docker:run": "docker run -p 3011:3011 fanz/ai-creator-assistant",
    "prompts:validate": "node scripts/validate-prompts.js",
    "models:download": "node scripts/download-models.js"
  },
  "keywords": [
    "ai",
    "creator-assistant",
    "content-generation",
    "revenue-optimization",
    "audience-analysis",
    "creative-tools",
    "chatbot",
    "recommendations"
  ],
  "author": "FANZ Engineering Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "socket.io": "^4.7.4",
    "bull": "^4.12.0",
    "node-cron": "^3.0.3",
    "@openai/openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@huggingface/inference": "^2.6.4",
    "openai": "^4.20.1",
    "langchain": "^0.0.208",
    "@langchain/openai": "^0.0.14",
    "@langchain/anthropic": "^0.1.0",
    "tiktoken": "^1.0.10",
    "replicate": "^0.25.2",
    "midjourney": "^1.0.0",
    "sharp": "^0.32.6",
    "canvas": "^2.11.2",
    "puppeteer": "^21.5.2",
    "cheerio": "^1.0.0-rc.12",
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "natural": "^6.8.0",
    "sentiment": "^5.0.2",
    "compromise": "^14.10.0",
    "franc": "^6.0.0",
    "crypto": "^1.0.1",
    "zlib": "^1.0.5",
    "stream": "^0.0.2",
    "path": "^0.12.7",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "cluster": "^0.7.7",
    "events": "^3.3.0",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/natural": "^5.1.5",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
