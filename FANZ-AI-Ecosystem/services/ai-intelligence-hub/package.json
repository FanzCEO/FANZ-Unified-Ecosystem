{
  "name": "@fanz/ai-intelligence-hub",
  "version": "1.0.0",
  "description": "Core AI processing engine for the FANZ ecosystem - intelligent content analysis, predictive analytics, automated moderation, and creator insights",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && copyfiles -u 1 src/**/*.json dist/",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:integration": "jest --testMatch='**/*.integration.test.ts' --passWithNoTests",
    "test:api": "jest --testMatch='**/*.api.test.ts' --passWithNoTests",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:content-analysis": "jest --testMatch='**/*content-analysis*.test.ts' --passWithNoTests",
    "test:predictions": "jest --testMatch='**/*prediction*.test.ts' --passWithNoTests",
    "benchmark": "node dist/utils/benchmark.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "clean": "rimraf dist",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "seed": "knex seed:run",
    "docker:build": "docker build -t fanz/ai-intelligence-hub .",
    "docker:run": "docker run -p 3010:3010 fanz/ai-intelligence-hub"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "content-analysis",
    "predictive-analytics",
    "moderation",
    "computer-vision",
    "nlp",
    "creator-economy"
  ],
  "author": "FANZ Engineering Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "socket.io": "^4.7.4",
    "bull": "^4.12.0",
    "node-cron": "^3.0.3",
    "@openai/openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@huggingface/inference": "^2.6.4",
    "@google-cloud/vision": "^4.0.1",
    "@google-cloud/language": "^6.0.1",
    "@google-cloud/translate": "^8.0.2",
    "aws-sdk": "^2.1490.0",
    "@aws-sdk/client-rekognition": "^3.456.0",
    "@aws-sdk/client-comprehend": "^3.456.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@tensorflow/tfjs": "^4.15.0",
    "sharp": "^0.32.6",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "crypto": "^1.0.1",
    "zlib": "^1.0.5",
    "stream": "^0.0.2",
    "path": "^0.12.7",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "cluster": "^0.7.7",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/parser": "^6.12.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "copyfiles": "^2.4.1",
