{
  "name": "@fanz/content-distribution-network",
  "version": "1.0.0",
  "description": "FANZ Multi-Platform Content Distribution Network with intelligent syndication and edge computing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fanz",
    "content-distribution",
    "cdn",
    "edge-computing",
    "content-syndication",
    "multi-platform",
    "video-streaming",
    "media-optimization"
  ],
  "author": "FANZ Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "aws-sdk": "^2.1489.0",
    "sharp": "^0.33.1",
    "ffmpeg-fluent": "^2.1.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}