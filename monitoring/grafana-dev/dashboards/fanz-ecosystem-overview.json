{
  "dashboard": {
    "id": null,
    "title": "üöÄ FANZ Unified Ecosystem - Overview Dashboard",
    "description": "Revolutionary Creator Economy Platform - System Overview",
    "tags": ["fanz", "ecosystem", "overview"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "üåü Platform Health Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up{job=\"fanz-api\"}",
            "legendFormat": "API Status",
            "refId": "A"
          },
          {
            "expr": "up{job=\"fanz-metaverse\"}",
            "legendFormat": "Metaverse Status",
            "refId": "B"
          },
          {
            "expr": "up{job=\"fanz-blockchain\"}",
            "legendFormat": "Blockchain Status",
            "refId": "C"
          },
          {
            "expr": "up{job=\"fanz-quantum\"}",
            "legendFormat": "Quantum AI Status",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.8
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "‚ùå Down"
                  },
                  "1": {
                    "text": "‚úÖ Up"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "üë• Active Users",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(fanz_active_users_total)",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        }
      },
      {
        "id": 3,
        "title": "üé≠ Total Creators",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(fanz_creators_total)",
            "legendFormat": "Creators",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "purple",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "title": "üí∞ Revenue Today",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(increase(fanz_revenue_total[24h]))",
            "legendFormat": "Revenue",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "green",
              "mode": "fixed"
            },
            "unit": "currencyUSD"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "title": "ü™ô Token Transactions",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(increase(fanz_token_transactions_total[1h]))",
            "legendFormat": "Transactions/Hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "fixedColor": "blue",
              "mode": "fixed"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 6,
        "title": "üìä API Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"fanz-api\"}[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true
        }
      },
      {
        "id": 7,
        "title": "‚ö° Response Time",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"fanz-api\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"fanz-api\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "show": true
          }
        ]
      },
      {
        "id": 8,
        "title": "üåê Metaverse Active Spaces",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "sum(fanz_metaverse_active_spaces)",
            "legendFormat": "Active Spaces",
            "refId": "A"
          },
          {
            "expr": "sum(fanz_metaverse_concurrent_users)",
            "legendFormat": "Concurrent Users",
            "refId": "B"
          }
        ]
      },
      {
        "id": 9,
        "title": "‚õìÔ∏è Blockchain Metrics",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 22
        },
        "targets": [
          {
            "expr": "fanz_blockchain_block_height",
            "legendFormat": "Block Height",
            "refId": "A"
          },
          {
            "expr": "rate(fanz_blockchain_transactions_total[5m])",
            "legendFormat": "Tx Rate",
            "refId": "B"
          }
        ]
      },
      {
        "id": 10,
        "title": "üî¨ Quantum AI Jobs",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 22
        },
        "targets": [
          {
            "expr": "sum(fanz_quantum_jobs_pending)",
            "legendFormat": "Pending Jobs",
            "refId": "A"
          },
          {
            "expr": "sum(fanz_quantum_jobs_running)",
            "legendFormat": "Running Jobs",
            "refId": "B"
          },
          {
            "expr": "rate(fanz_quantum_jobs_completed_total[5m])",
            "legendFormat": "Completion Rate",
            "refId": "C"
          }
        ]
      },
      {
        "id": 11,
        "title": "üíæ Database Performance",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "rate(postgres_stat_database_tup_inserted_total[5m])",
            "legendFormat": "Inserts/sec",
            "refId": "A"
          },
          {
            "expr": "rate(postgres_stat_database_tup_updated_total[5m])",
            "legendFormat": "Updates/sec",
            "refId": "B"
          },
          {
            "expr": "rate(postgres_stat_database_tup_deleted_total[5m])",
            "legendFormat": "Deletes/sec",
            "refId": "C"
          }
        ]
      },
      {
        "id": 12,
        "title": "üî• Redis Performance",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients",
            "refId": "B"
          },
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Memory Used (MB)",
            "refId": "C"
          }
        ]
      },
      {
        "id": 13,
        "title": "üö® Error Rate by Service",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 34
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"4..|5..\"}[5m]) / rate(http_requests_total[5m])",
            "legendFormat": "{{job}} Error Rate",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate",
            "max": 1,
            "min": 0,
            "unit": "percentunit"
          }
        ],
        "thresholds": [
          {
            "colorMode": "critical",
            "value": 0.05
          }
        ]
      },
      {
        "id": 14,
        "title": "üí° System Resource Usage",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 34
        },
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          },
          {
            "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100)",
            "legendFormat": "Disk Usage %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "max": 100,
            "min": 0,
            "unit": "percent"
          }
        ]
      },
      {
        "id": 15,
        "title": "üìà Creator Growth Metrics",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 40
        },
        "targets": [
          {
            "expr": "increase(fanz_new_creators_total[24h])",
            "legendFormat": "New Creators (24h)",
            "refId": "A"
          },
          {
            "expr": "increase(fanz_creator_verifications_total[24h])",
            "legendFormat": "Verifications (24h)",
            "refId": "B"
          }
        ]
      },
      {
        "id": 16,
        "title": "üí¨ Real-time Activity",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 40
        },
        "targets": [
          {
            "expr": "rate(fanz_messages_sent_total[5m])",
            "legendFormat": "Messages/sec",
            "refId": "A"
          },
          {
            "expr": "rate(fanz_tips_sent_total[5m])",
            "legendFormat": "Tips/sec",
            "refId": "B"
          },
          {
            "expr": "fanz_live_streams_active",
            "legendFormat": "Active Streams",
            "refId": "C"
          }
        ]
      },
      {
        "id": 17,
        "title": "üîí Security Metrics",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 40
        },
        "targets": [
          {
            "expr": "rate(fanz_failed_login_attempts_total[5m])",
            "legendFormat": "Failed Logins/sec",
            "refId": "A"
          },
          {
            "expr": "rate(fanz_rate_limit_exceeded_total[5m])",
            "legendFormat": "Rate Limits/sec",
            "refId": "B"
          },
          {
            "expr": "fanz_active_sessions_total",
            "legendFormat": "Active Sessions",
            "refId": "C"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(fanz_deployment_version[1h]) > 0",
          "iconColor": "green",
          "name": "Deployments",
          "titleFormat": "Deployment",
          "textFormat": "New version deployed"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "text": "All",
