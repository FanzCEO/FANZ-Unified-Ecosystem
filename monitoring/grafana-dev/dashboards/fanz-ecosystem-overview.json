          "hide": 0,
          "includeAll": true,
          "multi": true,
          "name": "service",
          "options": [],
          "query": "label_values(up{job=~\"fanz.*\"}, job)",
          "refresh": 1,
          "regex": "",
          "sort": 0,
          "type": "query"
        },
        {
