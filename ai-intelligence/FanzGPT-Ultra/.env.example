# FanzGPT Ultra Environment Configuration

# ============================================
# SERVER CONFIGURATION
# ============================================
NODE_ENV=development
PORT=3001
HOST=0.0.0.0

# CORS allowed origins (comma separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://boyfanz.com,https://girlfanz.com

# ============================================
# AI SERVICE CREDENTIALS
# ============================================
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
HUGGING_FACE_API_KEY=your_hugging_face_api_key_here

# AI Model Configuration
OPENAI_MODEL=gpt-4-turbo-preview
ANTHROPIC_MODEL=claude-3-opus-20240229
DEFAULT_MAX_TOKENS=2000

# ============================================
# DATABASE CONFIGURATION
# ============================================
DATABASE_URL=postgresql://username:password@localhost:5432/fanzgpt_ultra
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0

# ============================================
# SECURITY CONFIGURATION
# ============================================
JWT_SECRET=your_super_secure_jwt_secret_key_here
JWT_EXPIRES_IN=24h
ENCRYPTION_KEY=your_32_character_encryption_key_12
SIGNING_SECRET=your_signing_secret_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# ============================================
# AI MODEL SERVING
# ============================================
MODEL_SERVING_URL=http://localhost:8080
GPU_CLUSTER_ENDPOINT=your_gpu_cluster_endpoint_here
DEEPFAKE_MODEL_PATH=/models/deepfake-detection
SENTIMENT_MODEL_PATH=/models/sentiment-analysis

# ============================================
# CONTENT PROCESSING
# ============================================
MAX_CONTENT_SIZE=52428800
MAX_VIDEO_DURATION=600
IMAGE_QUALITY=80
VIDEO_QUALITY=720p

# File Storage
STORAGE_PROVIDER=s3
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=fanzgpt-ultra-storage

# ============================================
# MONITORING & LOGGING
# ============================================
LOG_LEVEL=info
LOG_FORMAT=combined
ENABLE_REQUEST_LOGGING=true

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development

# DataDog Configuration
DATADOG_API_KEY=your_datadog_api_key_here
DATADOG_APP_KEY=your_datadog_app_key_here
ENABLE_DATADOG=false

# Prometheus Configuration
PROMETHEUS_PORT=9090
PROMETHEUS_METRICS_PATH=/metrics

# ============================================
# FEATURE FLAGS
# ============================================
ENABLE_DEEPFAKE_DETECTION=true
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_CONTENT_GENERATION=true
ENABLE_BATCH_PROCESSING=true
ENABLE_REAL_TIME_OPTIMIZATION=true

# ============================================
# COMPLIANCE & REGIONAL SETTINGS
# ============================================
DEFAULT_REGION=US
ENABLE_GDPR_COMPLIANCE=true
ENABLE_CCPA_COMPLIANCE=true
CONTENT_RETENTION_DAYS=30
LOG_RETENTION_DAYS=90

# Age Verification
MIN_AGE_VERIFICATION=18
ENABLE_AGE_VERIFICATION=true
AGE_VERIFICATION_STRICT_MODE=true

# ============================================
# PERFORMANCE TUNING
# ============================================
WORKER_PROCESSES=auto
MAX_REQUEST_SIZE=50mb
REQUEST_TIMEOUT=30000
KEEP_ALIVE_TIMEOUT=5000

# Caching
CACHE_TTL=3600
ENABLE_REDIS_CACHE=true
CACHE_COMPRESSION=true

# ============================================
# THIRD-PARTY INTEGRATIONS
# ============================================

# Analytics
GOOGLE_ANALYTICS_ID=your_ga_id_here
MIXPANEL_TOKEN=your_mixpanel_token_here

# Communication
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key_here
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@example.com
SMTP_PASSWORD=your_email_password

# ============================================
# DEVELOPMENT & TESTING
# ============================================
DEBUG=fanzgpt:*
MOCK_AI_RESPONSES=false
ENABLE_SWAGGER_DOCS=true
API_DOCS_PATH=/docs

# Testing
TEST_DATABASE_URL=postgresql://username:password@localhost:5432/fanzgpt_ultra_test
TEST_REDIS_URL=redis://localhost:6380
DISABLE_AUTH_IN_TEST=true

# ============================================
# DEPLOYMENT CONFIGURATION
# ============================================
DOCKER_IMAGE_TAG=latest
KUBERNETES_NAMESPACE=fanzgpt-ultra
HELM_CHART_VERSION=1.0.0

# Health Check URLs
HEALTH_CHECK_URL=/health
READINESS_PROBE_URL=/ready
LIVENESS_PROBE_URL=/alive

# ============================================
# BACKUP & RECOVERY
# ============================================
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_ENCRYPTION_KEY=your_backup_encryption_key_here

# ============================================
# EXPERIMENTAL FEATURES
# ============================================
ENABLE_QUANTUM_AI=false
ENABLE_HOLOGRAPHIC_PROCESSING=false
ENABLE_BRAIN_COMPUTER_INTERFACE=false
ENABLE_METAVERSE_INTEGRATION=false