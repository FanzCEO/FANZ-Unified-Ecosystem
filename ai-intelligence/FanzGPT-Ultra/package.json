{
  "name": "fanzgpt-ultra",
  "version": "1.0.0",
  "description": "Revolutionary AI System for the FANZ Ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker build -t fanzgpt-ultra .",
    "docker:run": "docker run -p 3001:3001 fanzgpt-ultra"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "fanz",
    "creator-economy",
    "content-optimization",
    "adult-content",
    "deepfake-detection",
    "sentiment-analysis"
  ],
  "author": "FANZ Development Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "openai": "^4.52.1",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.3.1",
    "redis": "^4.6.14",
    "ioredis": "^5.4.1",
    "winston": "^3.13.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^10.0.0",
    "sharp": "^0.33.4",
    "ffmpeg-static": "^5.2.0",
    "node-cron": "^3.0.3",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "@tensorflow/tfjs-node": "^4.20.0",
    "@tensorflow/tfjs-node-gpu": "^4.20.0",
    "face-api.js": "^0.22.2",
    "sentiment": "^5.0.2",
    "compromise": "^14.14.0",
    "natural": "^6.12.0",
    "ml-matrix": "^6.11.1",
    "brain.js": "^2.0.0-beta.24"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.12",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.5.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FanzCEO/FANZ-Unified-Ecosystem.git",
    "directory": "ai-intelligence/FanzGPT-Ultra"
  },
  "bugs": {
    "url": "https://github.com/FanzCEO/FANZ-Unified-Ecosystem/issues"
  },
  "homepage": "https://github.com/FanzCEO/FANZ-Unified-Ecosystem#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}